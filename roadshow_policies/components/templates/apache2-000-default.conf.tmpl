# Webhuis default apache2 generated by template
#
# Do not edit
#
[%CFEngine any:: %]
<VirtualHost _default_:80>

  ServerAdmin webmaster@localhost

  DocumentRoot /var/www
  <Directory />
    Options FollowSymLinks
    AllowOverride None
    RedirectMatch ^/$ http://webhuis.nl
  </Directory>
  <Directory /var/www/>
    Options Indexes FollowSymLinks MultiViews
    AllowOverride None
    Order allow,deny
    allow from all
    RedirectMatch ^/$ http://webhuis.nl
  </Directory>

  ErrorLog ${apache2.apache_log_dir}/error.log

  LogLevel warn

  CustomLog ${apache2.apache_log_dir}/access.log combined

</VirtualHost>

