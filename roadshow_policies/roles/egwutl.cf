bundle agent egwutl {

vars:

  "bundles[igw_common]"     string => "$(sys.flavor)/egw_common.cf";
# "bundles[igw_data]"       string => "vlans.cf";

  "sequence"                 slist => getindices("bundles");
  "inputs"                   slist => getvalues("bundles");

methods:

  "Manage the gateway"   usebundle => "igw_common";

reports:

  "$(this.bundle): $(inputs)";

}
