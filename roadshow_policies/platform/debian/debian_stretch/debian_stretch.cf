bundle agent debian_stretch {

vars:

  "sources_list"      string => "deb http://ftp.ie.debian.org/debian/ stretch main contrib non-free
deb-src http://ftp.ie.debian.org/debian/ stretch main contrib non-free

deb http://security.debian.org/debian-security stretch/updates main
deb-src http://security.debian.org/debian-security stretch/updates main";

methods:

  "We desire the Stretch sources_list" usebundle => sources_list("$(sources_list)");

reports:

  "$(this.bundle):";

}

