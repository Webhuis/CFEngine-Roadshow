bundle agent status_device(device_parameter) {

classes:

  "stop"                 expression => regcmp( "bad", $(device_parameter) );

defaults:

  "device_status"            string => "good", if_match_regex => "bad";

methods:

 stop::
  "Stop IP processing"    usebundle => "stop_ip";

reports:

  "$(this.bundle): $(status)";

}
