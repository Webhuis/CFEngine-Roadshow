bundle agent get_mac_address(interface) {
  
classes:

  "ok" expression => regextract(
                                ".*HWaddr ([^\s]+).*(\n.*)*",
                                "$(interface)",
                                "mac");
vars:

  "mac_address"                        string => "$(mac[1])";

reports:

 "$(this.bundle): $(interface), $(mac_address)";

}

