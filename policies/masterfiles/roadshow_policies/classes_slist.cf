bundle agent classes_slist {

vars:

  "webservers"            slist => { "webapp0086", "webjen0190" };

classes:

  "webservers"              or  => { ${webservers} } ;

reports:

  webservers::
      "Hello, this host ${sys.fqhost} belongs to the webservers list: ${webservers}";

  !webservers::
      "Hello, this host ${sys.fqhost} does not belong to the webservers list: ${webservers}";

}
