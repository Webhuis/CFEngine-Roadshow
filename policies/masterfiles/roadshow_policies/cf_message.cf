bundle agent cf_message {

vars:
  "role"        string => "x2gvdt";

  "host_info"     data => parsejson('
      { "host": { "uqhost": "$(sys.uqhost)", "domain": "$(sys.domain)", "role": "$(role)" } }');

  "host_store"      string => storejson(host_info);

commands:

  "/root/cf-message.py ${host_store}";

reports:

  "${this.bundle} host_store: ${host_store}";

}
