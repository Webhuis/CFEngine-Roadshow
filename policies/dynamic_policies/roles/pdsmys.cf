bundle agent pdsmys {

vars:

      "bundles[${sys.flavor}_mysql_server_comp]"      string => "${sys.flavor}_mysql_server_comp.cf";
      "bundles[${sys.flavor}_pdns_server_comp]"       string => "${sys.flavor}_pdns_server_comp.cf";

      "sequence"  slist => getindices("bundles");
      "inputs"    slist => getvalues("bundles");

methods:

  "any" usebundle => "${sys.flavor}_mysql_server_comp";
  "any" usebundle => "${sys.flavor}_pdns_server_comp";

}
